#Selection sort
def ssort(list):
    for i in range(len(list)):
        index=i
        for j in range(i+1,len(list)):
            if list[j]<list[index]:
                index=j
        if index!=i:
            a=list[index]
            list[index]=list[i]
            list[i]=a
    return list
ssort([1,43,4,3,7])
            
